
    <div class="row justify-content-center">
        <div class="col-md-10">
            <h3> New documents </h3>
            <div class="trackContainer">
                <div class="list-group p-1" id="document_list"> 
                    <% @documents.each do |document| %>
                        <% if logged_in? && (document.routes.last.receiving_user == current_user || current_user.admin?) %>
                            <%= render document %>
                        <% end %>
                    <% end %>
                </div>
            </div>    
        </div>
    </div>


<div class="row justify-content-center">
  <div class="col-6">
    <h1>Pending documents</h1>
    <div class="trackContainer">
       <div class="list-group p-1">
            <% @documents.each do |document| %>
                <% if logged_in? && document.routes.last.receiving_user == current_user %>
                    <%= render 'document', document: document %>
                <% end %>
            <% end %>
        </div> 
    </div>
  </div>
  <div class="col-6">
    <h1>Pending documents</h1>
    <div class="trackContainer">
      <div id="docDetails"> 
      </div>
    </div>
  </div>
</div>

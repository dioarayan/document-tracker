<div class="container mt-3">
    <h1>Showing <%= @doctype.name %></h1>
    <%= link_to "Edit", edit_doctype_path(@doctype), class: "btn btn-primary" %>
    <%= link_to "Delete", doctype_path(@doctype), 
        data: { turbo_method: :delete, turbo_confirm: "Are you sure?"}, class: "btn btn-outline-danger" %>
    <%= link_to "Return", doctypes_path %>
</div>    
 
  <div class="row justify-content-center">
    <div class="col-12 p-3">
      <h4>Pending documents</h4>
      <div class="trackContainer">
        <div class="list-group p-1">
          <% Array(@routes).each do |route| %>
              <% if logged_in? && route.receiving_user == current_user %>
                  <%= render 'documents/document', document: route.document %>
              <% end %>
          <% end %>
        </div> 
      </div>  
    </div>
  </div>